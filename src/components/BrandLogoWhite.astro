---
import { Image } from "astro:assets";
import ajmLogo from '@/assets/logo-metalindo-white.png';

// Destructure and rename the "class" prop to "className"
const { class: className } = Astro.props;
const {id: idName} = Astro.props;

interface Props {
  class?: string;
  id?: string;
}
---

<Image src={ajmLogo} alt="AJM Logo White" id={idName} class={className} />